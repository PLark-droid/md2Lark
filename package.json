{
  "name": "md2lark",
  "version": "0.1.0",
  "description": "Markdown to Lark (Feishu) document converter - Miyabi powered autonomous project",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "npx tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"scripts/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist coverage",
    "lint": "eslint src tests scripts",
    "lint:fix": "eslint src tests scripts --fix",
    "build:extension": "npx tsx scripts/build-extension.ts",
    "build:extension:prod": "npx tsx scripts/build-extension.ts --prod"
  },
  "keywords": [
    "markdown",
    "lark",
    "feishu",
    "converter",
    "miyabi"
  ],
  "author": "PLark-droid",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PLark-droid/md2Lark.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.37",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.8.1",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.56.1"
  },
  "dependencies": {
    "marked": "^17.0.3"
  }
}
